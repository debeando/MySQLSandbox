{
  "datacenter": "MySQLSandbox",
  "data_dir": "/tmp/consul",
  "log_level": "warn",
  "enable_script_checks": true,
  "disable_update_check": true,
  "node_meta":
  {
    "service": "proxysql"
  },
  "retry_join":
  [
    "172.20.1.150"
  ],
  "services":[
    {
      "id":"proxysql",
      "name":"proxysql",
      "checks": [
        {
          "id":"port",
          "name":"Listen on port 3306",
          "tcp":"127.0.0.1:3306",
          "interval":"3s",
          "timeout":"1s"
        }
      ]
    }
  ],
  "watches": [
    {
      "type": "key",
      "key": "mysql/servers",
      "args": ["/root/handler.sh"]
    }
  ]
}
