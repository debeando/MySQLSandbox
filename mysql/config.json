{
  "datacenter": "MySQLSandbox",
  "data_dir": "/tmp/consul",
  "log_level": "warn",
  "enable_script_checks": true,
  "disable_update_check": true,
  "node_meta":
  {
    "service": "mysql"
  },
  "retry_join":
